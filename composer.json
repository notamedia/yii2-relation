    "relation",
    "many-to-many",
    "one-to-many"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/notamedia/yii2-relation/issues",
    "source": "https://github.com/notamedia/yii2-relation"
